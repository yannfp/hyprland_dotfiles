{
	"include": [
		"~/.config/waybar/config.d/modules.jsonc",
		"~/.config/waybar/config.d/custom_modules.jsonc",
		"~/.config/waybar/config.d/hypr_modules.jsonc"
	],

	// Position of the waybar
	"layer": "top",
	
	"height": 10,

	// Spacing between elemens of the waybar
	"spacing": 5,

	"modules-left": [
		"custom/date",
		"clock"
	],

	"modules-center": ["hyprland/workspaces"],

	"modules-right": [
		"network",
		"battery"
	],
}
